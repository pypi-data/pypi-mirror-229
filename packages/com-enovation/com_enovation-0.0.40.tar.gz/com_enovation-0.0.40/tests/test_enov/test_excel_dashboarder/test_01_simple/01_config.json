                  "format": {
                    "font_color": "red"
                  },
                  "multi_range": "@C:@C @D:@D"
                }
              },
              {
                "range": "@B:@B",
                "options": {
                  "type": "formula",
                  "criteria": "=RIGHT(@B@2,1)=\"A\"",
                  "format": {
                    "font_color": "red"
                  }
                }
              }
            ]
          },
