{
  "$id": "https://enovation.com/xlsxwriter/worksheet/set_row/parameters",
  "description": "To set properties for one or more rows of cells.",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "required": [
      "row"
    ],
    "properties": {
      "row": {
        "description": "The worksheet row (zero indexed).",
        "type": "integer",
        "min": 0
      },
      "height": {
        "description": "The row height, in character units.",
        "type": "number",
        "min": 0
      },
      "cell_format": {
        "description": "Optional Format object",
        "$ref": "/xlsxwriter/workbook/add_format/properties"
      },
      "options": {
        "description": "Optional parameters: hidden, level, collapsed",
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "level": {"type": "integer"},
          "hidden": {"type": "integer"},
          "collapsed": {"type": "integer"}
        }
      }
    }
  }
}