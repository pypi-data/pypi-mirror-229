#        '2001:0db8:1234:5678:9abc:def0:123f:fffd',
#        '2001:0db8:1234:5678:9abc:def0:123f:fffe'], dtype='<U39')

privat1 = generate_regex_from_ipv4_range(
    startip="192.168.0.0",
    endip="192.168.255.255",
    asbytes=True,
    compileregex=False,
